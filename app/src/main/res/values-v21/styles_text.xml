<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Headline.Black">
        <item name="android:fontFamily">sans-serif-black</item>
    </style>

    <style name="Title">
        <item name="android:textSize">@dimen/text_size_xh</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="Body2">
        <item name="android:textSize">@dimen/text_size_m</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="Subhead2">
        <item name="android:textSize">@dimen/text_size_h</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="OverLine">
        <item name="android:textSize">@dimen/text_size_xl</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0.16</item>
    </style>
</resources>